# Prints the path under apama-lib2 that binaries for this platform use.
# Consider calling getplatform.py directly instead of using this
#
# $Id: 0$
if [ -n "$PYTHONHOME" ] ; then 
	PYTHON="$PYTHONHOME/bin/python"
else 
	PYTHON=python
fi
"$PYTHON" `dirname $0`/getplatform.py get_ipath
